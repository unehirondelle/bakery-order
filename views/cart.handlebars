<h1>Your order:</h1>

<table>
    <th>
    <td>Name</td>
    <td>Size</td>
    <td>Quantity</td>
    <td>Price</td>
    <td>Your Comment</td>
    </th>
    {{#each orderlist}}
        <tr>
            <td>{{name}}</td>
            <td>{{size}}in</td>
            <td>{{quantity}}</td>
            <td>${{price}}</td>
            <td>{{comment}}</td>
        </tr>
    {{/each}}
</table>
<a href="/cakes"><button>Back to cakes list</button></a>